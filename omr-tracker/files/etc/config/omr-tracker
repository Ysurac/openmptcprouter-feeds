config defaults 'defaults'
	option enabled '1'
        list hosts '4.2.2.1'
        list hosts '8.8.8.8'
        list hosts '8.8.4.4'
        list hosts '9.9.9.9'
        list hosts '1.1.1.1'
        list hosts '1.0.0.1'
        list hosts '80.67.169.12'
        list hosts '80.67.169.40'
        option timeout '1'
        option tries '4'
        option interval '4'
        option type 'ping'
        option options ''

config shadowsocks 'shadowsocks'
	option enabled '1'
	list hosts '1.1.1.1'
	list hosts '1.0.0.1'
	list hosts '212.27.48.10'
	list hosts '23.96.52.53'

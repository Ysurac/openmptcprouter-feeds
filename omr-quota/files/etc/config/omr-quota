config interface 'wan1'
        option enabled '0'
        option txquota '100000'
        option rxquota '400000'
        option ttquota '500000'
        option interval '10'
        option interface 'wan1'

config interface 'wan2'
        option enabled '0'
        option txquota '100000'
        option rxquota '400000'
        option ttquota '500000'
        option interval '10'
        option interface 'wan2'

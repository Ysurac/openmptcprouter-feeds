# LBD configs
config LBD_SUPPORT_VHT160
	bool "Support VHT 160 MHz"
	depends on PACKAGE_kmod-qca-wifi-unified-profile||PACKAGE_kmod-qca-wifi-custc-profile||PACKAGE_kmod-qca-wifi-akronite-perf||PACKAGE_kmod-qca-wifi-dakota-perf||PACKAGE_kmod-qca-wifi-perf||PACKAGE_kmod-qca-wifi-unified-perf||PACKAGE_kmod-qca-wifi-lowmem-profile||PACKAGE_kmod-qca-wifi-flash_16mb-profile
	default y
	help
	  This enables the LBD support for 160 MHz

config LBD_SUPPORT_CFG80211
	bool "Support cfg80211 config layer for LBD"
	depends on PACKAGE_qca-cfg80211
	default y
	help
	  CFG80211 support for LBD

config LBD_SUPPORT_ATH10K
	bool "Support ATH10K"
	depends on PACKAGE_kmod-mac80211
	default y
	help
	  This enables the LBD support for Ath10k driver

config LBD_SUPPORT_QSDK
	bool "Support QSDK"
	depends on PACKAGE_kmod-qca-wifi-unified-profile||PACKAGE_kmod-qca-wifi-custc-profile||PACKAGE_kmod-qca-wifi-akronite-perf||PACKAGE_kmod-qca-wifi-dakota-perf||PACKAGE_kmod-qca-wifi-perf||PACKAGE_kmod-qca-wifi-unified-perf||PACKAGE_kmod-qca-wifi-lowmem-profile||PACKAGE_kmod-qca-wifi-flash_16mb-profile
	default y
	help
	  This enables the LBD support for Qualcomm Technologies Inc driver

config LBD_SUPPORT_CALLTRACE
        bool "Enable calltrace support for LBD"
        help
          calltrace support for LBD

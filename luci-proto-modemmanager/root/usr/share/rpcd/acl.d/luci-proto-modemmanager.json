{
	"luci-proto-modemmanager": {
		"description": "Grant access to mmcli",
		"read": {
			"cgi-io": [ "exec" ],
			"file": {
				"/usr/bin/mmcli -L -J": [ "exec" ],
				"/usr/bin/mmcli -m [0-9]* -J": [ "exec" ],
				"/usr/bin/mmcli -i [0-9]* -J": [ "exec" ],
				"/usr/bin/mmcli -m [0-9]* --location-get -J": [ "exec" ]
			}
		}
	}
}
